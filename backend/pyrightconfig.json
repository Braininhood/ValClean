{
  "include": [
    "apps",
    "config",
    "manage.py"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/migrations",
    "venv",
    ".venv"
  ],
  "venvPath": ".",
  "venv": "venv",
  "pythonVersion": "3.10",
  "typeCheckingMode": "basic",
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "reportImportCycles": false,
  "pythonPlatform": "Windows",
  "extraPaths": [
    "venv/Lib/site-packages"
  ],
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.10",
      "pythonPlatform": "Windows",
      "extraPaths": [
        "venv/Lib/site-packages"
      ]
    }
  ]
}
